{
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "plugins": [
    "eslint-plugin-tailwindcss"
  ],
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "tailwindConfig": "./tailwind.config.ts",
  "trailingComma": "all",
  "useTabs": false
}